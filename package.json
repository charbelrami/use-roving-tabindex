{
  "name": "use-roving-tabindex",
  "description": "Roving tabindex as a tiny (615B) react hook",
  "version": "0.1.1",
  "main": "dist/use-roving-tabindex.cjs",
  "module": "dist/use-roving-tabindex.mjs",
  "files": [
    "dist"
  ],
  "exports": {
    "import": "./dist/use-roving-tabindex.mjs",
    "require": "./dist/use-roving-tabindex.cjs"
  },
  "license": "MIT",
  "scripts": {
    "prod": "node scripts/cjs.mjs && node scripts/esm.mjs && gzip -c dist/use-roving-tabindex.mjs | wc -c",
    "preview": "node scripts/preview.mjs",
    "watch": "watchman-make -p 'index.js' 'preview.js' --run \"npm run preview\"",
    "serve": "node server.cjs",
    "prepare": "npm run prod",
    "test": "jest"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "esbuild": "^0.8.43",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "jest-playwright-preset": "^1.4.5",
    "playwright": "^1.8.1",
    "polka": "^0.5.2",
    "prettier": "^2.2.1",
    "serve-static": "^1.14.1"
  }
}
